{
  "extends": "airbnb",
  "rules": {
    "semi": 0,
    "comma-dangle": 0,
    "react/sort-comp": 0,
    "react/prop-types": 1,
    "react/no-multi-comp": 0,
    "react/jsx-no-bind": 0,
    "react/require-default-props": 0,
    "react/forbid-prop-types": 0,
    "no-case-declarations": 0,
    "arrow-parens": ["error", "always"],
    "consistent-return": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": 1,
    "no-plusplus": 0,
    "jsx-a11y/no-noninteractive-element-interactions": 0,
    "react/jsx-filename-extension": [
      "error", {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "import/no-extraneous-dependencies": 0,
    "import/default": 0,
    "import/no-duplicates": 0,
    "import/named": 0,
    "import/extensions": 0,
    "import/namespace": 0,
    "import/no-unresolved": 0,
    "import/no-named-as-default": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "keyword-spacing": 0,
    "max-len": ["error", {
      "code": 200,
      "ignoreComments": true,
      "ignoreTrailingComments": true
    }],
    "no-console": 0,
    "no-alert": 0
  },
  "plugins": [
    "react",
    "import"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures" : {
      "jsx": true
    }
  },
  "settings": {
    "import/resolve": {
      "moduleDirectory": ["node_modules", "src"]
    }
  },
  "globals": {
  }
}
